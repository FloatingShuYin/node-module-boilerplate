{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            },
            "modules": "auto",
            "useBuiltIns": "usage",
            "corejs": {
              "version": 3,
              "proposals": true
            }
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "useBuiltIns": "usage",
            "corejs": {
              "version": 3,
              "proposals": true
            }
          }
        ]
      ]
    }
  },
  "ignore": [
    "node_modules/**"
  ]
}
